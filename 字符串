print('hello')
b='hello%s'%'孙悟空'
print(b)
b='hello %s 你好 %s '%('tom','suwukong')
print(b)
#%3.5s字符串长度限制在三到五之间
b='hello%3.5s' %'12'
print(b)
#格式化字符串
a=123
c=f'hello {a} {b}'
print(c)
#创建一个变量保存你的名字
name='xxx'
#拼串
print('欢迎 '+name+' 光临')
#多个参数
print('欢迎',name,'光临')
#占位符
print('欢迎 %s 光临'%name)
#格式化字符串
print(f'欢迎 {name} 光临')
