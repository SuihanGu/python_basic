a = 1
b = 0
c = 0
while a < 10:
    while b < 10:
        while c < 10:
            if a**3+b**3+c**3 == a*100+b*10+c:
                print(a*100+b*10+c)
            c += 1
        b += 1
        c = 0
    a += 1
    b = 0
    c = 0
